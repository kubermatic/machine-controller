[Global]
user              = "admin"
password          = "password"
port              = ""
insecure-flag     = true
working-dir       = ""
datacenter        = ""
datastore         = ""
server            = ""
cluster-id        = ""

[Disk]
scsicontrollertype = "pvscsi"

[Workspace]
server            = "https://127.0.0.1:8443"
datacenter        = "Datacenter"
folder            = "some-folder"
default-datastore = "Datastore"
resourcepool-path = "/some-resource-pool"


[VirtualCenter "vc1"]
user = "1-some-user"
password = "1-some-password"
port = 443
datacenters = "1-foo"

[VirtualCenter "vc2"]
user = "2-some-user"
password = "2-some-password"
port = 443
datacenters = "2-foo"

